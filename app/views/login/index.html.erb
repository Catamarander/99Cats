
<ul>
<% @logins.each do |login| %>

  <li> <%= login.updated_at %></li>
  <li> Log me out of this session </li>
  <form id="form_id" action="login/whatever" method="post">
    <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />
    <input name="_method" type="hidden" value="delete">
    <input type="hidden" name="login[creation_time]" value="<%= login.updated_at %>">
    <input type='submit' values="Log out">
  </form>
  <br>

<% end %>
</ul>
